{
  "consumes": [
    "application/json",
    "multipart/form-data"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "http",
    "https"
  ],
  "swagger": "2.0",
  "info": {
    "description": "Документация Geo API.",
    "title": "Geo API.",
    "version": "1.0.0"
  },
  "basePath": "/",
  "paths": {
    "/api/address/geocode": {
      "post": {
        "security": [
          {
            "Bearer": [
              "[]"
            ]
          }
        ],
        "description": "Получение адреса по долготе и широте",
        "tags": [
          "geo"
        ],
        "operationId": "RequestGeocode",
        "parameters": [
          {
            "example": "{\"lat\":\"59.7221\",\"lng\":\"30.4554\"}",
            "description": "Координаты",
            "name": "Coordinates",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "ResponseGeocode",
            "schema": {
              "$ref": "#/definitions/ResponseGeocode"
            }
          }
        }
      }
    },
    "/api/address/search": {
      "post": {
        "security": [
          {
            "Bearer": [
              "[]"
            ]
          }
        ],
        "description": "Получение координат по адресу",
        "tags": [
          "geo"
        ],
        "operationId": "RequestAddress",
        "parameters": [
          {
            "example": "{\"query\": \"москва тверская\"}",
            "description": "Адрес",
            "name": "Query",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "ResponseAddress",
            "schema": {
              "$ref": "#/definitions/ResponseAddress"
            }
          }
        }
      }
    },
    "/api/login": {
      "post": {
        "description": "Авторизация пользователя",
        "tags": [
          "authorization"
        ],
        "operationId": "RequestLogin",
        "parameters": [
          {
            "example": "{\"name\":\"tim\",\"password\":\"123\"}",
            "description": "Авторизация",
            "name": "Authorization",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "ResponseLogin",
            "schema": {
              "$ref": "#/definitions/ResponseLogin"
            }
          }
        }
      }
    },
    "/api/register": {
      "post": {
        "description": "Регистрация нового пользователя",
        "tags": [
          "registration"
        ],
        "operationId": "RequestRegister",
        "parameters": [
          {
            "example": "{\"name\":\"tim\",\"password\":\"123\"}",
            "description": "Регистрация",
            "name": "Registration",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "ResponseRegister",
            "schema": {
              "$ref": "#/definitions/ResponseRegister"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Address": {
      "type": "object",
      "properties": {
        "lat": {
          "type": "string",
          "x-go-name": "Lat"
        },
        "lon": {
          "type": "string",
          "x-go-name": "Lng"
        },
        "value": {
          "type": "string",
          "x-go-name": "Value"
        }
      },
      "x-go-package": "test/geo"
    },
    "ResponseAddress": {
      "type": "object",
      "properties": {
        "Addresses": {
          "$ref": "#/definitions/Address"
        }
      },
      "x-go-package": "test/static"
    },
    "ResponseGeocode": {
      "type": "object",
      "properties": {
        "Addresses": {
          "$ref": "#/definitions/Address"
        }
      },
      "x-go-package": "test/static"
    },
    "ResponseLogin": {
      "type": "object",
      "properties": {
        "Token": {
          "description": "in:body\nToken содержит информацию о токене",
          "type": "string"
        }
      },
      "x-go-package": "test/static"
    },
    "ResponseRegister": {
      "type": "object",
      "properties": {
        "Token": {
          "description": "in:body\nToken содержит информацию о регистрации",
          "type": "string"
        }
      },
      "x-go-package": "test/static"
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  }
}